{
    "version": 2,
    "builds": [
        {
            "src": "./index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ],
    "cron": [
        {
            "path": "./api/scheduledTasks.js",
            "schedule": "*/1 * * * * *"
        }
    ]
}
