{
    "version": 2,
    "builds": [
        {
            "src": "./index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ],
    "cron": [
        {
            "src": "./api/scheduledTasks.js",
            "schedule": "*/2  * * * *"
        }
    ]
}
